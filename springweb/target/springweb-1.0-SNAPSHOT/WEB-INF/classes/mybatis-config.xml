<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--配置别名 -->
    <typeAliases>
        <!--定义Mapper中引用到的业务对象类的位置-->
        <package name="com.lh.course.user2.model" />
        <!--可以放多个-->
        <!--<package name="com.hbxy.course.xxx.model" />-->
    </typeAliases>
    <!--配置Mapper的位置 -->
    <mappers>
        <mapper resource="com/lh/course/user2/mapper/UserMapper.xml" />
        <!--可以放多个-->
        <!--<mapper resource="com/hbxy/course/user/mapper/XXXXMapper.xml" />-->
    </mappers>
</configuration>
